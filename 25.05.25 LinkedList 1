public class Node{
    Object item;
    Node next;

    Node(Object item){
        this.item = item;
    }

 /* 
    @Override
    public String toString(){
        return "Node{" + "item = " + item + ", next = "+ next + "}";
    }

    ì¶œë ¥ ê²°ê³¼:Node{item = A, next = Node{item = B, next = Node{item = C, next = null}}}
    ì´ë ‡ê²Œ ë‚˜ì˜¤ëŠ” ì´ìœ ëŠ” Node í´ë˜ìŠ¤ëŠ” ë‹¤ìŒ Node ê°ì²´ë¥¼ ê°€ì§€ê³  ìˆê¸°ì— ì¬ê·€ì ìœ¼ë¡œ ëŒì•„ê°€ë²„ë¦°ë‹¤.
*/

    @Override
    public String toString(){
        ë‚´ê°€ ê±´ë„ˆ ë›´ ë¶€ë¶„ì—ì„œ ì„¤ëª…ì„ í–ˆë‹¤ëŠ”ë° StringBuilderëŠ” ë³€ê²½í•  ìˆ˜ ìˆëŠ” Stringì´ë¼ ìƒê°í•˜ë©´ ëœë‹¤. ë¬¸ë²•ì€ append ë­ ì´ë ‡ê²Œ ìˆë‹¤.
        StringBuilder sb = new StringBuilder();
        Node x = this;
        sb.append("[");
        while (x != null){
            sb.append(x.item);
            if(x.next != null){
                sb.append("->");
            }
            
            x = x.next;
        }
        sb.append("]");
        return sb.toString();
        // ì¶œë ¥ê²°ê³¼: [A->B->C]
    }


  ë§ˆì§€ë§‰ ë…¸ë“œë¥¼ ê°€ì§€ê³  ì˜¤ëŠ”ê²ƒ 
        public static  Node getLastNode(Node node){ // ìŠ¤íƒœí‹±ì´ë¯€ë¡œ this ì‚¬ìš© ë¶ˆê°€ 
        Node x = node;
        while(x != null){
            if(x.next == null){
             
                return x;
            }
            x = x.next;
        }
        return null;
    }

    public static Node getNode(Node node, int index){
        Node x = node;
        for(int i = 0; i<index; i++){
            x = x.next;
        }

        return x;
    }


}


ë©”ì¸ì½”ë“œ
public class NodeMain {
    public static void main(String[] args) {
        Node first = new Node("A");
        first.next = new Node("B");
        first.next.next = new Node("C");


        //ëª¨ë“  ë…¸ë“œ íƒìƒ‰í•˜ê¸°
        System.out.println("ëª¨ë“  ë…¸ë“œ íƒìƒ‰í•˜ê¸°");
        Node x = first;
        while(x != null){
            System.out.println(x.item);
            x = x.next;
        }
        

        //ì—°ê²°ëœ ë…¸ë“œ íƒìƒ‰í•˜ê¸°
        System.out.println(first);
      


        //ë§ˆì§€ë§‰ ë…¸ë“œ íƒìƒ‰í•˜ê¸°
        Node lastNode = Node.getLastNode(first);
        System.out.println("lastNode = "+lastNode);

        //íŠ¹ì • ì¸ë±ìŠ¤ì˜ ë…¸ë“œ ì¡°íšŒí•˜ê¸°
        int index = 2;
        Node indexNode = Node.getNode(first, index);
        System.out.println("indexNode = "+ indexNode); 
        
    }
}
